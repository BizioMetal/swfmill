<?xml version="1.0" encoding="iso-8859-1"?>

<movie width="220" height="270" framerate="15">
	<background color="#dddddd"/>

		<library>
			<clip import="classes.swf"/>
			<clip id="jpg" import="library/testjpg.jpg"/>
			<clip id="png" import="library/testpng.png"/>
			<clip id="png-alpha" import="library/testpng-alpha.png"/>
			<clip id="swf" class="org.swfmill.Foo" import="library/star.swf"/>
		</library>
		<font id="vera" import="library/vera.ttf"
			glyphs="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890 ÄÖÜäöüß .:!"/>

	<frame>
		<place id="jpg" x="10" y="10" depth="1" scale="0.5"/>
		<textfield id="jpgtext" 
			width="60" height="15" 
			font="vera" size="12"
			text="JPG"
			/>
		<place id="jpgtext" x="10" y="60" depth="2"/>
	
		<place id="png" x="80" y="10" depth="3" scale="0.5"/>
		<textfield id="pngtext" 
			width="60" height="15" 
			font="vera" size="12"
			text="PNG"
			/>
		<place id="pngtext" x="80" y="60" depth="4"/>
	
		<place id="png-alpha" x="150" y="10" depth="5" scale="0.5"/>
		<textfield id="pngatext" 
			width="150" height="15" 
			font="vera" size="12"
			text="PNG(A)"
			/>
		<place id="pngatext" x="150" y="60" depth="6"/>
	
	
		<place id="swf" x="0" y="80" depth="7" scale="10" name="star"/>
		<set name="star" member="text" value="Hello World!"/>
		<textfield id="swftext" 
			width="150" height="15" 
			font="vera" size="12"
			text="SWF"
			/>
		<place id="swftext" x="10" y="140" depth="8"/>
	
	
		<textfield id="text" 
			width="110" height="50" 
			font="vera" size="12"
			text="the quick brown fox jumps over the lazy dog"
			/>
		<place id="text" x="100" y="85" depth="9"/>
		<textfield id="texttext" 
			width="100" height="15" 
			font="vera" size="12"
			text="Font"
			/>
		<place id="texttext" x="100" y="140" depth="10"/>

		<textfield id="scriptout" 
			width="200" height="50" 
			font="vera" size="10"
			text=""
			/>
		<place id="scriptout" name="scriptout" x="10" y="180" depth="11"/>

		<call object="Main" method="main"/>
	</frame>
	
	<html-wrapper name="test.html" swf="output.swf"/>
</movie>
